{
  "button": {
    "icon": {
      "padding": {
        "x": { "$*": 4 },
        "y": { "$*": 4 }
      },
      "borderRadius": { "$*": 4 },
      "icon": {
        "size": {
          "x": { "$*": 32 },
          "y": { "$*": 32 }
        }
      },
      "stroke": {
        "color": {
          "$*": { "r": 0.4, "g": 0.4, "b": 0.4 },
          "$hover": { "r": 0.2, "g": 0.4, "b": 0.9 }
        },
        "width": { "$*": 1 },
        "alpha": { "$*": 1 }
      }
    },
    "iconVertical": {
      "padding": {
        "x": { "$*": 8 },
        "y": { "$*": 8 }
      },
      "borderRadius": { "$*": 4 },
      "icon": {
        "size": {
          "x": { "$*": 32 },
          "y": { "$*": 32 }
        }
      },
      "iconGap": { "$*": 6 },
      "stroke": {
        "color": {
          "$*": { "r": 0.4, "g": 0.4, "b": 0.4 },
          "$hover": { "r": 0.2, "g": 0.4, "b": 0.9 }
        },
        "width": { "$*": 1 },
        "alpha": { "$*": 1 }
      },
      "label": {
        "fontSize": { "$*": 11 },
        "color": { "$*": { "r": 0.2, "g": 0.2, "b": 0.2 } },
        "alpha": { "$*": 1 }
      }
    },
    "text": {
      "padding": {
        "x": { "$*": 16 },
        "y": { "$*": 8 }
      },
      "borderRadius": { "$*": 4 },
      "fill": {
        "color": {
          "$*": { "r": 0.2, "g": 0.5, "b": 0.8 },
          "$hover": { "r": 0.3, "g": 0.6, "b": 0.9 }
        },
        "alpha": {
          "$*": 1,
          "$disabled": 0.5
        }
      },
      "label": {
        "fontSize": { "$*": 14 },
        "color": { "$*": { "r": 1, "g": 1, "b": 1 } },
        "alpha": {
          "$*": 1,
          "$disabled": 0.5
        }
      }
    },
    "inline": {
      "padding": {
        "x": { "$*": 12 },
        "y": { "$*": 8 }
      },
      "borderRadius": { "$*": 6 },
      "iconGap": { "$*": 8 },
      "icon": {
        "size": {
          "x": { "$*": 20 },
          "y": { "$*": 20 }
        },
        "alpha": {
          "$*": 1,
          "$disabled": 0.5
        }
      },
      "fill": {
        "color": {
          "$*": { "r": 0.15, "g": 0.65, "b": 0.45 },
          "$hover": { "r": 0.1, "g": 0.8, "b": 0.6 }
        },
        "alpha": {
          "$*": 1,
          "$disabled": 0.5
        }
      },
      "stroke": {
        "color": {
          "$hover": { "r": 0.2, "g": 0.4, "b": 0.9 }
        },
        "width": {
          "$hover": 1
        },
        "alpha": {
          "$hover": 1
        }
      },
      "label": {
        "fontSize": { "$*": 14 },
        "color": { "$*": { "r": 1, "g": 1, "b": 1 } },
        "alpha": {
          "$*": 1,
          "$disabled": 0.5
        }
      }
    },
    "primary": {
      "text": {
        "fill": {
          "color": {
            "$*": { "r": 0.8, "g": 0.2, "b": 0.2 },
            "$hover": { "r": 0.9, "g": 0.3, "b": 0.3 }
          }
        }
      }
    }
  }
}

