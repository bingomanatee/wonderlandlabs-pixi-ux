{
  "button": {
    "padding": {
      "$*": { "x": 4, "y": 4 }
    },
    "icon": {
      "$*": { "size": { "x": 80, "y": 80 }, "alpha": 1 },
      "$hover": { "size": { "x": 96, "y": 96 }, "alpha": 1 }
    },
    "iconVertical": {
      "padding": {
        "$*": { "x": 6, "y": 10 }
      },
      "icon": {
        "$*": { "size": { "x": 70, "y": 72 }, "alpha": 1 },
        "$hover": { "size": { "x": 74, "y": 76 }, "alpha": 1 }
      }
    },
    "text": {
      "padding": {
        "$*": { "x": 12, "y": 6 }
      },
      "label": {
        "fontSize": {
          "$*": 10
        }
      }
    }
  }
}
