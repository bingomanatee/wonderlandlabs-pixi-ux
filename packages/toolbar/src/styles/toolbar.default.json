{
  "button": {
    "icon": {
      "$*": {
        "size": { "x": 80, "y": 80 },
        "alpha": 1
      },
      "$hover": {
        "size": { "x": 80, "y": 80 },
        "alpha": 1
      },
      "$disabled": {
        "size": { "x": 80, "y": 80 },
        "alpha": 0.5
      }
    },
    "padding": {
      "$*": { "x": 4, "y": 4 }
    },
    "stroke": {
      "$*": {
        "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
        "alpha": 1,
        "width": 1
      },
      "$hover": {
        "color": { "r": 0.7, "g": 0.7, "b": 0.7 },
        "alpha": 1,
        "width": 1
      },
      "$disabled": {
        "color": { "r": 0.85, "g": 0.85, "b": 0.85 },
        "alpha": 0.5,
        "width": 1
      }
    },
    "fill": {
      "$disabled": {
        "color": { "r": 0.9, "g": 0.9, "b": 0.9 },
        "alpha": 0.3
      }
    },
    "inline": {
      "padding": {
        "$*": { "x": 12, "y": 6 }
      },
      "borderRadius": {
        "$*": 4
      },
      "iconGap": {
        "$*": 8
      },
      "icon": {
        "$*": {
          "size": { "x": 16, "y": 16 },
          "alpha": 1
        },
        "$hover": {
          "size": { "x": 16, "y": 16 },
          "alpha": 1
        },
        "$disabled": {
          "size": { "x": 16, "y": 16 },
          "alpha": 0.5
        }
      },
      "fill": {
        "$*": {
          "color": { "r": 0.33, "g": 0.67, "b": 0.6 },
          "alpha": 1
        },
        "$hover": {
          "color": { "r": 0.27, "g": 0.73, "b": 0.53 },
          "alpha": 1
        },
        "$disabled": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0.5
        }
      },
      "stroke": {
        "$*": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0,
          "width": 0
        },
        "$hover": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0,
          "width": 0
        },
        "$disabled": {
          "color": { "r": 0.67, "g": 0.67, "b": 0.67 },
          "alpha": 1,
          "width": 2
        }
      },
      "label": {
        "fontSize": {
          "$*": 13
        },
        "color": {
          "$*": { "r": 1, "g": 1, "b": 1 },
          "$hover": { "r": 1, "g": 1, "b": 1 },
          "$disabled": { "r": 0.7, "g": 0.7, "b": 0.7 }
        },
        "alpha": {
          "$*": 1,
          "$hover": 1,
          "$disabled": 0.5
        }
      }
    },
    "text": {
      "padding": {
        "$*": { "x": 12, "y": 6 }
      },
      "borderRadius": {
        "$*": 4
      },
      "fill": {
        "$*": {
          "color": { "r": 0.33, "g": 0.67, "b": 0.6 },
          "alpha": 1
        },
        "$hover": {
          "color": { "r": 0.27, "g": 0.73, "b": 0.53 },
          "alpha": 1
        },
        "$disabled": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0.5
        }
      },
      "stroke": {
        "$*": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0,
          "width": 0
        },
        "$hover": {
          "color": { "r": 0.5, "g": 0.5, "b": 0.5 },
          "alpha": 0,
          "width": 0
        },
        "$disabled": {
          "color": { "r": 0.67, "g": 0.67, "b": 0.67 },
          "alpha": 1,
          "width": 2
        }
      },
      "label": {
        "fontSize": {
          "$*": 13
        },
        "color": {
          "$*": { "r": 1, "g": 1, "b": 1 },
          "$hover": { "r": 1, "g": 1, "b": 1 },
          "$disabled": { "r": 0.7, "g": 0.7, "b": 0.7 }
        },
        "alpha": {
          "$*": 1,
          "$hover": 1,
          "$disabled": 0.5
        }
      }
    },
    "iconVertical": {
      "padding": {
        "$*": { "x": 4, "y": 4 }
      },
      "icon": {
        "$*": {
          "size": { "x": 80, "y": 80 },
          "alpha": 1
        },
        "$hover": {
          "size": { "x": 80, "y": 80 },
          "alpha": 1
        },
        "$disabled": {
          "size": { "x": 80, "y": 80 },
          "alpha": 0.5
        }
      },
      "label": {
        "fontSize": {
          "$*": 12
        },
        "padding": {
          "$*": 4
        },
        "color": {
          "$*": { "r": 0, "g": 0, "b": 0 },
          "$hover": { "r": 0, "g": 0, "b": 0 }
        },
        "alpha": {
          "$*": 0.5,
          "$hover": 1
        }
      }
    }
  }
}
